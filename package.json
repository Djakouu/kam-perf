{
  "name": "lighthouse-dashboard-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "api",
    "worker",
    "web"
  ],
  "scripts": {
    "install:all": "npm install && npm install -w api -w worker -w web",
    "dev:api": "npm run dev -w api",
    "dev:worker": "npm run dev -w worker",
    "dev:web": "npm run dev -w web",
    "dev": "npm run dev:api & npm run dev:worker & npm run dev:web",
    "prisma:generate": "npx prisma generate --schema=./prisma/schema.prisma",
    "prisma:push": "npx prisma db push --schema=./prisma/schema.prisma",
    "prisma:seed": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts"
  },
  "devDependencies": {
    "prisma": "^5.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/luxon": "^3.7.1",
    "date-fns": "^4.1.0",
    "luxon": "^3.7.2",
    "react-router-dom": "^7.11.0"
  }
}
